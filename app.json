{
  "expo": {
    "name": "Allge_Care",
    "slug": "Allge_Care",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logoAllge-Care.png",
    "scheme": "allgecare",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/logoAllge-Care.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/iconosComponentes/1.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.sora13507.Allge_Care"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-secure-store"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "9f713476-359f-4388-a242-6cb0e930eb97"
      }
    }
  }
}
